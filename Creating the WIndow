import pygame
pygame.init()

pygame.display.set_caption('Car Racing Game')
display_width = 800
display_height = 600
gamedisplay = pygame.display.set_mode((display_width, display_height))
clock-pygame.time.Clock()

def game_loop():
    



running = True
while running:
    pygame.display.flip()
    for event in pygame.event.get():
        if event.type == pygame.QUIT:
            running = False
